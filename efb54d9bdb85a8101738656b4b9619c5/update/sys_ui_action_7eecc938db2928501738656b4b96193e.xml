        <name>Approve</name>
        <onclick/>
        <order>100</order>
        <script><![CDATA[var gr = new GlideRecord('sysapproval_approver');
gr.addQuery('document_id',current.sys_id);
gr.addQuery('state', 'requested');
gr.addQuery('approver',gs.getUserID());
gr.query();

var checkApprovalPermission = gr.getRowCount() > 0;

if(checkApprovalPermission == true){
	while(gr.next()){
		gr.state = 'approved';
		gr.update();
	}
}
else{
	gs.addErrorMessage("You don't have permission to approve this record!");
}
]]></script>
